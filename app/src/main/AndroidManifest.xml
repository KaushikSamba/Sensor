<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kaushiksamba.sensor" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/sensor_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".sensor_default"
            android:label="@string/title_activity_sensor_default"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kaushiksamba.sensor.MainActivity" />
        </activity>
        <activity
            android:name=".ProximityGraphic"
            android:label="@string/title_activity_proximity_graphic"
            android:parentActivityName=".sensor_default" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kaushiksamba.sensor.sensor_default" />
        </activity>

    </application>

</manifest>
